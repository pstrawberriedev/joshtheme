<header>
  <nav class="white">
    <ul id="slide-out" class="side-nav">
      {{#foreach navigation}}
      <li class="{{slug}}{{#if current}} active{{/if}}" role="presentation">
        <a class="waves-effect" href="{{url absolute="true"}}">{{label}}</a>
      </li>
      {{/foreach}}
    </ul>
    <div class="nav-wrapper">
      <a href="{{url absolute="true"}}" class="brand-logo">{{meta_title}}</a>
      <ul class="right hide-on-med-and-down">
        {{#foreach navigation}}
          <li class="{{slug}}{{#if current}} active{{/if}}" role="presentation">
            <a class="waves-effect" href="{{url absolute="true"}}">{{label}}</a>
          </li>
        {{/foreach}}
      </ul>
      <a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>
    </div>
  </nav>

</header>