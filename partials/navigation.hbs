<header role="main">
 
  <nav role="main">
    <div class="logo">
      <a href="{{url absolute="true"}}" class="brand-logo">{{meta_title}}</a>
    </div>
  </nav>
  <ul id="mobile-nav" role="menubar" aria-hidden="true">
    {{#foreach navigation}}
      <li class="{{slug}}{{#if current}} active{{/if}}" role="menuitem">
        <a href="{{url absolute="true"}}">{{label}}</a>
      </li>
    {{/foreach}}
  </ul>
  <div id="mobile-menu" tabindex="1">
    <span class="typcn typcn-th-menu"></span>
  </div>
  
</header>